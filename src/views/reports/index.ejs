<!DOCTYPE html>
<html>

<head>
    <% include ../static/partials/head.ejs %>
</head>

<body>
    <main class="container">
        <% include ../static/partials/navbar.ejs %>

        <a href="/reports/new" class="btn btn-success">New Report</a>
        <h1>Reports</h1>

        <ul class="list-group">
            <% reports.forEach((report) => { %>
            <li class="list-group-item">
                <a href="/reports/<%= report.id %>"> <%= report.date.toDateString() %> </a>
            </li>
            <% }) %>
        </ul>

    </main>
</body>

</html>