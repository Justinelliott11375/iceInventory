<!DOCTYPE html>
<html>

<head>
    <% include ../static/partials/head.ejs %>
</head>

<body>
    <% include ../static/partials/navbar.ejs %>

    <main class="container">
        <a href="/reports/<%= amIceInventory.reportId %>/amIceInventories/<%= amIceInventory.id %>/edit"
            class="btn btn-warning">Edit</a>
        <form style="display:initial"
            action="/reports/<%= amIceInventory.reportId %>/amIceInventories/<%= amIceInventory.id %>/destroy"
            method="post">
            <button type="submit" class="btn btn-danger">Delete</button>
        </form>
        <a href="/reports/<%= amIceInventory.reportId %>" class="btn btn-success">Back to Report</a>
        <ul class="list-group">
            <li class="list-group-item">
                <%= amIceInventory.amount %>
                <%= amIceInventory.unit %>
                <%= amIceInventory.product %>
            </li>
        </ul>
    </main>

    <% include ../static/partials/baseScripts.ejs %>

</body>

</html>